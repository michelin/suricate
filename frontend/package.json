    "angular2-grid": "^2.1.0",
    "cerialize": "^0.1.18",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "material-design-icons-iconfont": "^3.0.3",
    "ng2-stomp-service": "^1.2.2",
    "ng2-validation": "^4.2.0",
    "ngx-color-picker": "^5.3.3",
    "rxjs": "^5.5.2",
    "sockjs-client": "^1.1.4",
    "stompjs": "^2.3.3",
    "zone.js": "^0.8.14"
  },
