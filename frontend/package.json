{
  "name": "suricate-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "yarn run webpack:dev --port=4200",
    "start-docker": "yarn webpack:docker --port=4200",
    "build": "webpack --config webpack/webpack.dev.js",
    "build-docker": "rimraf dist && webpack --config webpack/webpack.docker.js --env=docker --progress --profile --bail",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "webpack:dev": "yarn run webpack-dev-server -- --config webpack/webpack.dev.js --progress --inline --hot --profile --watch-content-base",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack:docker": "yarn run webpack-dev-server -- --config webpack/webpack.docker.js --progress --inline --hot --profile --watch-content-base"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.6",
